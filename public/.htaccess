# Proteger archivo .env
<Files .env>
    Order allow,deny
    Deny from all
</Files>

# Opcional: proteger composer.json y composer.lock
<FilesMatch "^(composer\.json|composer\.lock)$">
    Order allow,deny
    Deny from all
</FilesMatch>

# Opcional: proteger archivos de configuraci√≥n
<FilesMatch "^config.php$">
    Order allow,deny
    Deny from all
</FilesMatch>
